<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="100" name="Mobile" parallel="tests">

  <test name="AndroidAppTest 1">
    <parameter name="version" value="15" />
    <parameter name="platform" value="Android" />
    <parameter name="device" value="Galaxy *" />
    <classes>
      <class name="com.lambdaTestApp.android.AndroidApp" />
    </classes>
  </test>

  <test name="AndroidAppTest 2">
    <parameter name="version" value="12" />
    <parameter name="platform" value="Android" />
    <parameter name="device" value="Galaxy S*" />
    <classes>
      <class name="com.lambdaTestApp.android.AndroidApp" />
    </classes>
  </test>
</suite>